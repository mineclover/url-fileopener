{
  "name": "fopen-cli",
  "version": "1.0.0",
  "packageManager": "pnpm@10.14.0",
  "license": "MIT",
  "description": "CLI tool for opening files via custom URL scheme",
  "bin": {
    "fopen": "./dist/bin-simple.cjs",
    "fopen-handler": "./dist/bin/fopen-handler-simple.cjs"
  },
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && pnpm copy-package-json",
    "clean": "rimraf dist/*",
    "test": "vitest run",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "protocol-registry": "github:mineclover/protocol-registry"
  }
}